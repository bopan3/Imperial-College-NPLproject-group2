M58（"室女座A"）, 
M59（"室女座E"）, 
M60（"室女座E"）, 
M61（"星云 S"）, 
M84（"室女座A"）, 
M85（"室女座A"）, 
M86（"室女座A"）, 
M87（"室女座A"）, 
M88（"室女座A"）, 
M89（"室女座A"）, 
M90（"室女座A"）, 
M91（"室女座A"）, 
M98（"室女座A"）, 
M99（"室女座A"）, 
