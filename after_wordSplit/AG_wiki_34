def   genkey ( length ) :
def   xor _ strings ( s , t ) :
message   =   ' This   is   a   secret   message '
print ( ' message : ' ,   message )
key   =   genkey ( len ( message ) )
print ( ' key : ' ,   key )
cipherText   =   xor _ strings ( message ,   key )
print ( ' cipherText : ' ,   cipherText )
print ( ' decrypted : ' ,   xor _ strings ( cipherText , key ) )
if   xor _ strings ( cipherText ,   key )   = =   message :
