OUTPUT   =   " Hello ,   world ! "
END
create   table   MESSAGE   ( TEXT   char ( 15 ) ) ;
insert   into   MESSAGE   ( TEXT )   values   ( ' Hello ,   world ! ' ) ;
select   TEXT   from   MESSAGE ;
drop   table   MESSAGE ;
select   ' hello ,   world ' ;
print   ' hello , world ! '
